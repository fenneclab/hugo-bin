{
  "name": "hugo-bin",
  "version": "0.20.0",
  "hugoVersion": "0.34",
  "description": "Binary wrapper for Hugo",
  "repository": "fenneclab/hugo-bin",
  "author": "satoshun00 <shun.sato@fenneclab.com>",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "hugo": "cli.js"
  },
  "dependencies": {
    "bin-wrapper": "3.0.2",
    "del-cli": "1.1.0",
    "logalot": "2.1.0"
  },
  "devDependencies": {
    "bin-check": "4.1.0",
    "eslint": "4.10.0",
    "lodash": "4.17.4",
    "mocha": "4.0.1"
  },
  "scripts": {
    "test": "eslint . && mocha",
    "postinstall": "del-cli vendor && node lib/install"
  },
  "files": [
    "lib/",
    "index.js",
    "cli.js"
  ],
  "engines": {
    "node": ">=4"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "mocha": true
    },
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
