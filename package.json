{
  "name": "hugo-bin",
  "version": "0.17.0",
  "hugoVersion": "0.30.2",
  "description": "Binary wrapper for Hugo",
  "repository": "fenneclab/hugo-bin",
  "author": "satoshun00 <shun.sato@fenneclab.com>",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "hugo": "cli.js"
  },
  "dependencies": {
    "bin-wrapper": "3.0.2",
    "del-cli": "0.2.1",
    "logalot": "2.1.0"
  },
  "devDependencies": {
    "bin-check": "4.0.1",
    "eslint": "3.8.1",
    "lodash": "4.16.4",
    "mocha": "3.1.2"
  },
  "scripts": {
    "test": "eslint . && mocha",
    "postinstall": "del-cli vendor && node lib/install"
  },
  "files": [
    "lib/",
    "index.js",
    "cli.js"
  ],
  "engines": {
    "node": ">=4"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "mocha": true
    },
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
