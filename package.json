{
  "name": "hugo-bin",
  "version": "0.34.0",
  "hugoVersion": "0.46",
  "description": "Binary wrapper for Hugo",
  "repository": "fenneclab/hugo-bin",
  "author": "satoshun00 <shun.sato@fenneclab.com>",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "hugo": "cli.js"
  },
  "dependencies": {
    "bin-wrapper": "^4.0.0",
    "pkg-conf": "^2.1.0",
    "rimraf": "^2.6.2",
    "signale": "^1.3.0"
  },
  "devDependencies": {
    "bin-check": "^4.1.0",
    "eslint": "^5.6.1",
    "mocha": "^5.2.0"
  },
  "scripts": {
    "eslint": "eslint .",
    "mocha": "mocha",
    "test": "npm run eslint && npm run mocha",
    "postinstall": "rimraf vendor && node lib/install"
  },
  "files": [
    "lib/*.js",
    "index.js",
    "cli.js"
  ],
  "engines": {
    "node": ">=6"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
